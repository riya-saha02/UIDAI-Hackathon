    normalized_name = normalized_name.replace('mumbai city', 'mumbai')\n","    normalized_name = normalized_name.replace('mumbai suburban', 'mumbai')\n","    normalized_name = normalized_name.replace('north twenty four parganas', 'north 24 parganas')\n","    normalized_name = normalized_name.replace('south twenty four parganas', 'south 24 parganas')\n","    normalized_name = normalized_name.replace('k v rangareddy', 'rangareddy')\n","    normalized_name = normalized_name.replace('y s r', 'ysr')\n","    normalized_name = normalized_name.replace('n t r', 'ntr')\n","    normalized_name = normalized_name.replace('sri potti sriramulu nellore', 'sri potti sriramulu nellore')\n","    normalized_name = normalized_name.replace('sas nagar mohali', 'mohali')\n","    normalized_name = normalized_name.replace('bangalore rural', 'bengaluru rural')\n","    normalized_name = normalized_name.replace('bangalore urban', 'bengaluru urban')\n","    normalized_name = normalized_name.replace('medchalmalkajgiri', 'medchal malkajgiri')\n","    normalized_name = re.sub(r'\\s*\\*', '', normalized_name)\n","    normalized_name = re.sub(r'\\s+', ' ', normalized_name).strip()\n","\n","    return normalized_name\n","\n","# Apply the normalization function to each unique district list\n","normalized_biometric_districts = [normalize_district_name(d) for d in unique_biometric_districts]\n","normalized_demographic_districts = [normalize_district_name(d) for d in unique_demographic_districts]\n","normalized_enrolment_districts = [normalize_district_name(d) for d in unique_enrolment_districts]\n","\n","print(\"First 10 normalized biometric districts:\", normalized_biometric_districts[:10])\n","print(\"\\nFirst 10 normalized demographic districts:\", normalized_demographic_districts[:10])\n","print(\"\\nFirst 10 normalized enrolment districts:\", normalized_enrolment_districts[:10])"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["First 10 normalized biometric distric